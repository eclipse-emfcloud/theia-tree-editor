{
  "private": true,
  "name": "electron-app",
  "version": "0.0.0",
  "dependencies": {
    "@theia/core": "^1.0.0",
    "@theia/editor": "^1.0.0",
    "@theia/electron": "^1.0.0",
    "@theia/filesystem": "^1.0.0",
    "@theia/markers": "^1.0.0",
    "@theia/messages": "^1.0.0",
    "@theia/monaco": "^1.0.0",
    "@theia/navigator": "^1.0.0",
    "@theia/plugin-ext": "^1.0.0",
    "@theia/plugin-ext-vscode": "^1.0.0",
    "@theia/preferences": "^1.0.0",
    "@theia/process": "^1.0.0",
    "@theia/terminal": "^1.0.0",
    "@theia/workspace": "^1.0.0",
    "tree-editor-example": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "^1.0.0"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "download:plugins": "theia download:plugins",
    "clean": "theia clean",
    "build": "theia build --mode development && yarn download:plugins",
    "start": "theia start --port=3000 --loglevel=debug --plugins=local-dir:./plugins --root-dir=../workspace",
    "watch": "theia build --watch --mode development"
  },
  "theia": {
    "target": "electron"
  }
}
